{
  "name": "@mayajs/router",
  "version": "0.6.4",
  "description": "Simple NodeJS routing library created using Typescript",
  "main": "lib/index.js",
  "scripts": {
    "rimraf": "rimraf interface && rimraf index.js && rimraf index.d.ts && rimraf utils && rimraf router && rimraf lib && rimraf class && rimraf exceptions && rimraf types",
    "clean": "npm run rimraf && npm run tgz",
    "tgz": "rimraf *.tgz",
    "start": "npm run clean && tsc -w",
    "prepack": "npm run clean && tsc --outDir .",
    "postpack": "npm run clean"
  },
  "keywords": [
    "nodejs",
    "server",
    "router",
    "typescript",
    "library",
    "REST",
    "API",
    "mayajs"
  ],
  "author": "Mark Anthony C. Ignacio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayajs/router.git"
  },
  "bugs": {
    "url": "https://github.com/mayajs/router/issues"
  },
  "homepage": "https://github.com/mayajs/router#readme",
  "devDependencies": {
    "@types/node": "^13.11.0",
    "jest": "^25.2.7",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "interface",
    "utils",
    "types",
    "class",
    "exceptions",
    "router",
    "**.d.ts"
  ]
